#include<iostream>
using namespace std;
int main()
{
	int n;
	int arr[n],i;
	int max,min;
	cout<<"enter size";
	cin>>n;
	cout<<"enter array elements";
	for(i=0;i<n;i++)
	{
		cin>>arr[i];
		
	}
	max=arr[0];
	min=arr[0];
	for(i=1;i<n;i++)
	{
		if(arr[i]>max)
		{
			max=arr[1];
		}
		
        if(arr[i]<min)
{
	min=arr[i];
	}
	}


cout<<max<<endl;
cout<<min<<endl;	
	return 0;
} 
