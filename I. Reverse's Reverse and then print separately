#include<iostream>
using namespace std;
int main()
{
	int n,sum=0,r,x;
	
cout<<"enter a number";
cin>>n;
while(n!=0)
{
	r=n%10;
	sum =sum*10+r;
	n=n/10;
}
cout<<sum<<endl;
while(sum!=0)
{
	x=sum%10;
	switch(x)
	{
		case 1:cout<<"one";
			break;
		case 2: cout<<"two";
		break;
		case 3: cout<<"threee";
		break;
		
	}
	cout<<x<<endl;
	sum=sum/10;
}

	return 0;
}
