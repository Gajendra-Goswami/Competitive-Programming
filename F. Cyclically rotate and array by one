#include<iostream>
using namespace std;
int main()
{
	int last;int i;int a[100];
	int n;
	cout<<"enter no of elemets";
	cin>>n;
	cout<<"enter array";
	for(int i=0;i<n;i++)
	{
		cin>>a[i];
	}
	last=a[n-1];
	for(i=n-1;i>0;i--)
	{
		a[i]=a[i-1];
		
		
			}
			a[0]=last;
			cout<<"printing array\n";
			for(int i=0;i<n;i++)
			{
				cout<<a[i]<<" ";
				
			}
	
	return 0;
}
